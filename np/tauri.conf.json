{
  "build": {
    "beforeBuildCommand": "cd ./client && npm run build",
    "beforeDevCommand": "cd ./client && npm run dev",
    "frontendDist": "../client/build",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Now Playing desktop widget",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "np",
  "version": "0.0.17",
  "identifier": "io.github.gyng",
  "plugins": {},
  "app": {
    "macOSPrivateApi": false,
    "windows": [
      {
        "fullscreen": false,
        "height": 400,
        "resizable": true,
        "title": "NowPlaying",
        "width": 300,
        "transparent": true,
        "decorations": false
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}